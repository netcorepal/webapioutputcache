<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net45</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>netcorepal</Authors>
    <Company>netcorepal</Company>
    <Description>outputcache for asp.net webapi</Description>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/netcorepal/webapioutputcache</PackageProjectUrl>
    <RepositoryUrl>https://github.com/netcorepal/webapioutputcache.git</RepositoryUrl>
    <Version>1.0.0</Version>
    <PackageTags>netcorepal webapi outputcache cache</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\net45\NetCorePal.WebApi.OutputCache.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\net45\NetCorePal.WebApi.OutputCache.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.WebApi.Core" Version="5.2.3" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.Web" />
  </ItemGroup>

</Project>
